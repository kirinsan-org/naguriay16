{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "start",
      "args": [
        "start"
      ],
      "isBuildCommand": true
    },
    {
      "taskName": "tsc",
      "args": [
        "run",
        "tsc"
      ]
    },
    {
      "taskName": "tsc:w",
      "args": [
        "run",
        "tsc:w"
      ]
    },
    {
      "taskName": "supervisor",
      "args": [
        "run",
        "supervisor"
      ]
    },
    {
      "taskName": "browserSync",
      "args": [
        "run",
        "browserSync"
      ]
    }
  ]
}